package chap04_controlstatement;

public class _09_DuplicatedFor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        //1. 2~9단까지 구구단 출력
		for(int i = 2; i <=9; i++) {
			for(int j = 1; j <= 9; j++) {
				System.out.print(i + " * " + j + " = " + (i * j) + "    ");
			}			// 1번 실행하고 줄 변경
			System.out.println();
		}
		
		//2. * 세로 5줄 가로 5줄
		for(int x = 1; x < 6; x++) {			for(int z = 1; z < 6; z++) {				System.out.print(" * ");			}			System.out.println();		}
			
		
		
		//3. AB + BA = 99를 만족하는 A값과 B값을 모두 출력하세요. (A, B는 0~9까지의 정수)				for(int i = 0; i <= 9; i++) {						for(int t = 0; t <= 9; t++) {										if((i * 10 + t) + (t * 10 + i) == 99) {																										System.out.print("i의 값 : " + i);								System.out.println("t의 값 : " + t);								System.out.println("-------------");				}								}				}
		
	
		
	}

}
